{
  "name": "puter-cursor-ai",
  "version": "1.0.0",
  "description": "Advanced AI-powered code editor based on Cursor AI with Puter.js integration",
  "main": "dist/main/main.js",
  "homepage": "https://github.com/puter-ai/puter-cursor",
  "author": "Puter AI Team",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "webpack --mode development --config webpack.main.config.js && electron dist/main/main.js",
    "dev:renderer": "webpack serve --mode development --config webpack.renderer.config.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --mode production --config webpack.main.config.js",
    "build:renderer": "webpack --mode production --config webpack.renderer.config.js",
    "start": "electron dist/main/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
